{
  "name": "promise-synchronizer",
  "description": "synchronize promise",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fisker/promise-synchronizer.git"
  },
  "bugs": {
    "url": "https://github.com/fisker/promise-synchronizer/issues"
  },
  "version": "1.1.1",
  "main": "lib/index.common.js",
  "module": "lib/index.mjs",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "rimraf lib",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier **/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml} --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint **/*.md --ignore \"**/node_modules/**\"",
    "release": "run-s clean lint build format test dist",
    "test": "ava --verbose",
    "test-coverage": "nyc yarn test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "dependencies": {
    "deasync": "^0.1.15",
    "p-is-promise": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@fisker/commitlint-config": "^1.1.0",
    "@fisker/eslint-config": "^1.2.6",
    "@fisker/eslint-config-ava": "^1.2.0",
    "@fisker/husky-config": "^1.1.0",
    "@fisker/lint-staged-config": "^1.0.4",
    "@fisker/prettier-config": "^1.0.17",
    "ava": "^2.2.0",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^6.0.1",
    "esm": "^3.2.25",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "markdownlint-cli": "^0.17.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.17.0",
    "rollup-plugin-filesize": "^6.1.1",
    "rollup-plugin-prettier": "^0.6.0"
  },
  "keywords": [
    "promise",
    "promise-synchronizer",
    "sync",
    "synchronize",
    "synchronizer"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "ava": {
    "require": "esm"
  },
  "nyc": {
    "require": "esm"
  },
  "sideEffects": false
}
